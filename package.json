{
  "name": "danielepelagatti.com",
  "description": "A static responsive threejs site generated using grunt tasks, jade, coffeescript, compass and markdown",
  "destination_host": "http://www.danielepelagatti.com",
  "www_folder": "www",
  "watch_folder": "src",
  "coffee_folder": "src/coffee",
  "percolator_main": "main.coffee",
  "compiled_js": "src/js/optional/main.js",
  "compass_folder": "src/compass",
  "compass_output_folder": "www/css",
  "glsl_folder": "src/glsl",
  "glsl_output_file": "src/js/optional/shaders.js",
  "markdown_folder": "src/markdown",
  "jade_folder": "src/jade",
  "default_document": "index.html",
  "js_folder": "src/js",
  "minified_vendor_js_file": "www/js/vendor.min.js",
  "minified_main_js_file": "www/js/main.min.js",
  "minified_main_css_input_file": "www/css/main.css",
  "minified_main_css_file": "www/css/main.min.css",
  "config_json": "www/config.json",
  "devDependencies": {
    "grunt-contrib-compass": "^0.7.2",
    "grunt-contrib-coffee": "^0.10.1",
    "grunt-contrib-jade": "^0.11.0",
    "grunt-contrib-uglify": "^0.4.0",
    "grunt-contrib-watch": "^0.6.0",
    "grunt": "^0.4.4",
    "coffee-script": "^1.7.1",
    "uglify-js": "^2.4.13",
    "jade": "^1.3.0",
    "compass": "^0.1.0",
    "grunt-cli": "^0.1.13",
    "grunt-coffee-percolator-v2": "^0.2.4",
    "grunt-glsl-threejs": "~0.1.2",
    "grunt-concurrent": "~0.5.0",
    "grunt-contrib-connect": "~0.7.1",
    "connect-livereload": "~0.3.2",
    "http-rewrite-middleware": "~0.1.5",
    "markdown-js": "0.0.3",
    "marked": "~0.3.2",
    "meta-marked": "~0.3.1",
    "grunt-modernizr": "~0.5.2",
    "grunt-contrib-cssmin": "~0.9.0",
    "grunt-contrib-copy": "^0.5.0",
    "grunt-rsync": "^0.5.0"
  }
}
